msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator %1 is available."
msgstr "LibreELEC USB-SD Creator %1 e disponibil."

msgctxt "Creator|"
msgid "Select project"
msgstr "Selectați proiectul"

msgctxt "Creator|"
msgid "Select image"
msgstr "Selectați imaginea"

msgctxt "QObject|"
msgid "Write Error"
msgstr "Eroare la scriere"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"O eroare a apărut la încercarea de a scrie datele.\n"
"Eroare %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Eroare volum"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"O eroare a apărut la încercarea de a accesa volumul.\n"
"Eroare %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Eroare blocare"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"O eroare a apărut la încercarea de a bloca volumul.\n"
"Eroare %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Eroare la deblocare"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"O eroare a apărut la încercarea de a debloca volumul.\n"
"Eroare %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Eroare la demontare"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"O eroare a apărut la încercarea de a demonta volumul.\n"
"Eroare %1: %2"
