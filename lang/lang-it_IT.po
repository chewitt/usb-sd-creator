msgctxt "QObject|"
msgid "Write Error"
msgstr "Errore scrittura"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Si e' verificato un errore durante la scrittura del dato nell'handle.\n"
"Errore %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Errore nel volume"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Si è verificato un errore durante l'apertura di un handle nel volume.\n"
"Errore %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Errore blocco volume"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Si è verificato un errore durante il blocco del volume.\n"
"Errore %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Errore sblocco volume"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Si è verificato un errore durante lo sblocco del volum.\n"
"Errore %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Errore smontaggio voume"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Si è verificato un errore durante l'operazione di smontaggio del volume.\n"
"Errore %1: %2"
