msgid "Download complete, syncing file..."
msgstr ""
"Download completato.\n"
"Sincronizzazione file..."

msgctxt "Creator|"
msgid "Download complete, verifying checksum..."
msgstr ""
"Download completato.\n"
"Verifica checksum..."

msgctxt "Creator|"
msgid "Download complete, checksum ok."
msgstr ""
"Download completato.\n"
"Checksum OK."

msgctxt "Creator|"
msgid "Download complete, checksum not ok."
msgstr ""
"Download completato.\n"
"Checksum NON ok."

