msgid "Error: %1"
msgstr "Грешка: %1"

#, qt-format
msgctxt "Creator|"
msgid ""
"Selected device: %1\n"
"Are you sure you want to write the image?\n"
"\n"
"Your USB-SD device will be wiped!"
msgstr ""
"Одавран уређај: %1\n"
"Да ли сте сигурни да желите да запишете слику?\n"
"\n"
"Сви подаци са USB-SD диска ће бити избисани!"

msgctxt "Creator|"
msgid "Yes"
msgstr "Да"

msgctxt "Creator|"
msgid "No"
msgstr "Не"

msgctxt "Creator|"
msgid "OK"
msgstr "ОК"

msgctxt "Creator|"
msgid "Update Notification"
msgstr "Освежи обавештења"

msgctxt "Creator|"
msgid "Select version"
msgstr "Одаберите верзију"

msgctxt "Creator|"
msgid "&Close"
msgstr "Затвори"

msgctxt "Creator|"
msgid "&Download"
msgstr "Преузимање"

msgctxt "Creator|"
msgid "&About"
msgstr "О пројекту"

msgctxt "Creator|"
msgid "&Write"
msgstr "Запиши"

msgctxt "Creator|"
msgid "&Help"
msgstr "Помоћ"

msgctxt "Creator|"
msgid "Select &file"
msgstr "Одаберите фајл"

msgctxt "Creator|"
msgid "&Show all"
msgstr "Прикажи све"

msgctxt "Creator|"
