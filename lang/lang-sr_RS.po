msgctxt "Creator|"
msgid "Not enough space on %1 [%2 < %3]"
msgstr "Нема довољно простора на %1 [%2 < %3]"

msgctxt "Creator|"
msgid "Syncing file system..."
msgstr "Синхронизујем фајл сисзем."

msgctxt "Creator|"
msgid "Writing done!"
msgstr "Записивање завршено!"

#, qt-format
